<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>ASCIIExtractor</servlet-name>
		<servlet-class>asciiextractor.ASCIIExtractorServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ASCIIExtractor</servlet-name>
		<url-pattern>/asciiextractor</url-pattern>
	</servlet-mapping>
	
	
		
	<servlet>
		<servlet-name>ResultTracker</servlet-name>
		<servlet-class>asciiextractor.ResultTracker</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ResultTracker</servlet-name>
		<url-pattern>/resulttracker</url-pattern>
	</servlet-mapping>
	
			<servlet>
		<servlet-name>DominantColorPicker</servlet-name>
		<servlet-class>asciiextractor.DominantColorPicker</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DominantColorPicker</servlet-name>
		<url-pattern>/color</url-pattern>
	</servlet-mapping>
	
		<servlet>
		<servlet-name>Users</servlet-name>
		<servlet-class>asciiextractor.Users</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Users</servlet-name>
		<url-pattern>/users</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
		<servlet-name>Visit</servlet-name>
		<servlet-class>asciiextractor.Visit</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Visit</servlet-name>
		<url-pattern>/visit</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Afflatus</servlet-name>
		<servlet-class>asciiextractor.Afflatus</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Afflatus</servlet-name>
		<url-pattern>/afflatus</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
